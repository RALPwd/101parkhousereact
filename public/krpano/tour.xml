	</scene>

<action name="showfoto">
		if(layer[jacuzzi].show,
			set(layer[jacuzzi].enabled, false);
			tween(layer[jacuzzi].alpha, 0, 0.5);
			tween(layer[jacuzzi].scale, 0, 0.2);
			set(layer[jacuzzi].show, false);
			skin_showthumbs(true);
		,
			set(layer[jacuzzi].enabled, true);
			tween(layer[jacuzzi].alpha, 1.0, 0.5);
			tween(layer[jacuzzi].scale, 1.0, 0.5);
			set(layer[jacuzzi].show, true);
			skin_showthumbs(false);
		);
</action>		


